<div>
  <h2 class="btn btn-primary" data-toggle="collapse" href="#burn_context" role="button" aria-controls="collapse">Burn Context:</h2>
  <div class="collapse" id="burn_context">
    <% field.values.each do |value| %>
      <input type="checkbox" name="call[details[burn_context][]" value="<%= value %>">
      <label><%= value %></label>
    <% end %>
  </div>
</div>
