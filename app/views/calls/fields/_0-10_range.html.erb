<fieldset class="mb-3">
  <legend class="form-call-legend"><%= field.name.humanize %></legend>
  <div class = "form-call form-call-range">
    <% 11.times do |n| %>
      <label>
      <input type="radio" name="call[details[<%= field.name %>]" id="<%= n %>" value="<%= n %>">
        <div>
          <%= n %>
        </div>
      </label>
    <% end %>
  </div>
</fieldset>
